   "source": [
    "def set_systems(M, r):\n",
    "    \"\"\"\n",
    "    generates all set systems of size r\n",
    "    \"\"\"\n",
    "    groundset = list(M.groundset())\n",
    "    ss = tuple(powerset(groundset))\n",
    "    for _ in range(r):\n",
    "        ss = tuple(product(ss, powerset(groundset)))\n",
    "    \n",
    "    return ss\n",
    "\n",
    "len(list(set_systems(M, 3)))"
   ]
  }
